{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "vue": ["vue"],
      "models": ["models"],
      "api": ["api"],
      "plugins": ["plugins"],
      "pages": ["pages"]
    }
  },
  "compileOnSave": false
}
